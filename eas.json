{
  "cli": {
    "version": ">= 7.0.0",
    "appVersionSource": "local" 
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "channel": "production", 
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      },
      "cache": {
        "disabled": true
      }
    },
    "production": {
      "android": {
        "buildType": "apk" 
      },
      "channel": "production",
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      }
    }
  },
  "submit": {
    "production": {}
  }
}